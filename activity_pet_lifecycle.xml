<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PetLifecycle_Activity">

    <ImageView
        android:id="@+id/graph"
        android:layout_width="291dp"
        android:layout_height="570dp"
        android:layout_marginTop="152dp"
        android:layout_marginEnd="58dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/graph" />

    <ImageView
        android:id="@+id/new_age"
        android:layout_width="86dp"
        android:layout_height="70dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="104dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/new_age" />

    <ImageView
        android:id="@+id/tran_age"
        android:layout_width="72dp"
        android:layout_height="73dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="292dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tran_age" />

    <ImageView
        android:id="@+id/social_age"
        android:layout_width="99dp"
        android:layout_height="73dp"
        android:layout_marginTop="360dp"
        android:layout_marginEnd="88dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/social_age" />

    <ImageView
        android:id="@+id/teen_age"
        android:layout_width="105dp"
        android:layout_height="68dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="436dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/teen_age" />

    <ImageView
        android:id="@+id/youth_age"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:layout_marginTop="500dp"
        android:layout_marginEnd="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/youth_age" />

    <ImageView
        android:id="@+id/adult_age"
        android:layout_width="85dp"
        android:layout_height="73dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="576dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/adult_age" />

    <ImageView
        android:id="@+id/old_age"
        android:layout_width="64dp"
        android:layout_height="60dp"
        android:layout_marginTop="620dp"
        android:layout_marginEnd="120dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/old_age" />

    <Button
        android:id="@+id/Nur_Btn"
        android:layout_width="120dp"
        android:layout_height="57dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="96dp"
        android:backgroundTint="#ffe8ed"
        android:text="양육정보"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Heal_Btn"
        android:layout_width="120dp"
        android:layout_height="57dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="96dp"
        android:backgroundTint="#ffe8ed"
        android:text="건강정보"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="241dp"
        android:layout_height="58dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="80dp"
        android:text="생애 주기별 정보"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>